{
  "name": "remark-obsidian-link",
  "version": "0.2.2",
  "description": "turn obsidian links into markdown links",
  "author": "Austin Gatlin",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "default": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "repository": "aegatlin/remark-obsidian-link",
  "keywords": [],
  "license": "ISC",
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "test": "node --test",
    "format": "prettier --write src test.js README.md",
    "prepare": "skooh",
    "ppp": "npm version patch && git push && npm publish"
  },
  "hooks": {
    "pre-commit": "npm run format",
    "pre-push": "npm run test"
  },
  "dependencies": {
    "mdast-builder": "^1.1.1",
    "remark-wiki-link": "^1.0.4",
    "unist-util-visit": "^4.1.1"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "remark": "^14.0.2",
    "skooh": "^0.2.1",
    "tsup": "^6.7.0",
    "typescript": "^4.8.4"
  }
}
